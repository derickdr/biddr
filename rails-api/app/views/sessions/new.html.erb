<div>
    <div>
        <h2>Please Sign In</h1>
    </div>
    <div>
        <%= form_tag(sessions_path, method: "POST") do %>
        <div>
        <%= label_tag :username %>
        <%= text_field_tag :username, '', placeholder: 'username123' %>
        </div>
        <div>
        <%= label_tag :password %>
        <%= password_field_tag :password, '', placeholder: 'password' %>
        </div>
        <div>
        <%= submit_tag "Login" %>
        </div>
    <% end %>
    </div>
</div>
